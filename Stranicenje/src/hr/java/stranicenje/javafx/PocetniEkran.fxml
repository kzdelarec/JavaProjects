<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<BorderPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hr.java.stranicenje.javafx.PocetniEkranController">
   <center>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Region prefHeight="100.0" prefWidth="31.0" />
            <Button fx:id="fifo" mnemonicParsing="false" onAction="#prikaziEkranZaFifo" prefHeight="50.0" prefWidth="100.0" text="FIFO" />
            <Region prefHeight="100.0" prefWidth="31.0" />
            <Button fx:id="rlu" mnemonicParsing="false" onAction="#prikaziEkranZaLfu" prefHeight="50.0" prefWidth="100.0" text="LFU" />
            <Region prefHeight="100.0" prefWidth="31.0" />
            <Button fx:id="rfu" mnemonicParsing="false" onAction="#prikaziEkranZaLru" prefHeight="50.0" prefWidth="100.0" text="LRU" />
            <Region prefHeight="100.0" prefWidth="31.0" />
         </children>
      </HBox>
   </center>
</BorderPane>
